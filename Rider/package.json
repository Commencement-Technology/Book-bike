{
  "name": "Rider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:jetify": "npx jetify",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "test:lint": "eslint . --ext .js,.ts,.tsx",
    "test:code-formatter": "prettier app/**/**/*.tsx --write",
    "test:code-formatter-step-1": "prettier --check app/**/*.tsx",
    "test:code-formatter-step-2": "prettier --check app/**/**/*.tsx",
    "test:code-formatter-step-3": "prettier --check app/**/**/**/*.tsx",
    "test:types-check": "tsc --project . --noEmit --pretty --skipLibCheck",
    "test:staged-types-check": "bash -c tsc --project . --noEmit --pretty --skipLibCheck",
    "android:build": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release"
  },
  "dependencies": {
    "@dooboo-ui/native-switch-toggle": "^0.7.0",
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-native-firebase/app": "^11.2.0",
    "@react-native-firebase/messaging": "^11.2.0",
    "@react-native-picker/picker": "^1.9.11",
    "@react-navigation/bottom-tabs": "^5.11.9",
    "@react-navigation/compat": "^5.3.11",
    "@react-navigation/material-top-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "@timwangdev/react-native-geocoder": "^1.0.0-alpha.5",
    "@types/fbemitter": "^2.0.32",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-svg-charts": "^5.0.7",
    "apisauce": "^2.0.1",
    "formik": "^2.2.6",
    "husky": "^4.3.8",
    "i18n-js": "^3.8.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "lodash.debounce": "^4.0.8",
    "ramda": "^0.27.1",
    "re-reselect": "^4.0.0",
    "react": "16.13.1",
    "react-native": "0.75.2",
    "react-native-android-location-enabler": "^1.2.2",
    "react-native-appearance": "^0.3.4",
    "react-native-background-timer": "^2.4.1",
    "react-native-calendar-strip": "^2.2.1",
    "react-native-config": "^1.4.2",
    "react-native-date-picker": "^3.2.10",
    "react-native-device-info": "^8.1.3",
    "react-native-fast-image": "^8.3.4",
    "react-native-fs": "^2.18.0",
    "react-native-geolocation-service": "^5.2.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-google-places-autocomplete": "^2.1.3",
    "react-native-image-picker": "^3.6.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-localize": "^2.0.1",
    "react-native-maps": "0.27.1",
    "react-native-maps-directions": "^1.8.0",
    "react-native-modal": "^11.6.1",
    "react-native-otp-inputs": "^6.0.0",
    "react-native-permissions": "^3.0.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^1.13.2",
    "react-native-responsive-fontsize": "^0.5.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.1",
    "react-native-svg": "^12.1.1-0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-tab-view": "^2.16.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.2.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "socket.io-client": "^3.1.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.0",
    "@types/lodash": "^4.14.168",
    "@types/ramda": "^0.27.35",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.46",
    "@types/react-native-keyboard-spacer": "^0.4.1",
    "@types/react-redux": "^7.1.15",
    "@types/redux-logger": "^3.0.8",
    "@types/seamless-immutable": "^7.1.15",
    "babel-jest": "^29.7.0",
    "eslint": "^7.18.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.4.4",
    "typescript": "3.9.2"
  },
  "lint-staged": {
    "*.tsx": [
      "yarn test:lint",
      "yarn test:code-formatter",
      "yarn test:code-formatter-step-1",
      "yarn test:code-formatter-step-2",
      "yarn test:code-formatter-step-3",
      "yarn test:staged-types-check"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
